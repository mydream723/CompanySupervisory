<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_background_login"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    android:padding="@dimen/tools_padding_default" >

    <TextView
        android:id="@+id/tv_unbindserviceDialog_title"
        style="@style/font_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/unbindserviceDialog_title" />

    <ImageView
        android:id="@+id/iv_unbindserviceDialog_img"
        android:layout_width="@dimen/tools_pic_size_default"
        android:layout_height="@dimen/tools_pic_size_default"
        android:layout_gravity="center"
        android:layout_margin="@dimen/tools_padding_default"
        android:src="@drawable/ic_launcher" />

    <Button
        android:id="@+id/bt_unbindserviceDialog_unbind"
        style="@style/button_dialog_default"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/unbindserviceDialog_unbind" />

    <Button
        android:id="@+id/bt_unbindserviceDialog_cancel"
        style="@style/button_dialog_default"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/tools_padding_small"
        android:text="@string/unbindserviceDialog_cancel" />

</LinearLayout>